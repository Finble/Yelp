<%= form_tag("/restaurants", method: "post") do %>
<%= label_tag(:name, "Restaurant name:") value:(@restaurant.name) %>
<%= text_field_tag(:name) %>
<%= label_tag(:rating, "Restaurant rating:") %>
<%= radio_button_tag(:rating, "1") %>
<%= radio_button_tag(:rating, "2") %>
<%= radio_button_tag(:rating, "3") %>
<%= radio_button_tag(:rating, "4") %>
<%= radio_button_tag(:rating, "5") %>
<%= submit_tag("Add Restaurant") %>
<% end %>
